    }
  ],

  "require":{
    "php": ">=5.3"
  },

  "require-dev":{
    "phpunit/phpunit":"3.7.*",
    "cocur/background-process": "^0.7.0"
  },

  "autoload":{
    "psr-4":{
      "alexandervas\\slackbotlistener\\":"src/"
